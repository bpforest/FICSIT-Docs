<link rel="icon" href="{{siteRootPath}}/favicon.ico" type="image/x-icon">

<meta name="description" content="{{#if page.description}}{{detag page.description}}{{else}}{{#unless (eq page.layout '404')}}{{{og-description page.contents}}}{{/unless}}{{/if}}">
<meta property="og:title" content="{{{detag (or page.title defaultPageTitle)}}}{{#if site.title}} :: {{site.title}}{{/if}}">
{{#if page.attributes.og-image}}
<meta property="og:image" content="{{og-image page}}">
{{else}}
{{#if page.contents}}
{{#with (getFirstImage page.contents)}}
<meta property="og:image" content="{{this}}">
{{else}}
<meta property="og:image" content="{{site.url}}/logo.png">
{{/with}}
{{else}}
<meta property="og:image" content="{{site.url}}/logo.png">
{{/if}}
{{/if}}
<meta property="og:description" content="{{#if page.description}}{{detag page.description}}{{else}}{{#unless (eq page.layout '404')}}{{{og-description page.contents}}}{{/unless}}{{/if}}">

<link rel="stylesheet" href="{{uiRootPath}}/css/site-extra.css">
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@docsearch/css@3" />

<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/9.15.10/styles/androidstudio.min.css" integrity="sha256-YSbArrnKaJ05w0kASSAYJu/S174kd3l1wbGarhjTd5g=" crossorigin="anonymous" />

<meta name="docsearch:component_name" content="{{ @root.page.componentVersion.name }}"/>
<meta name="docsearch:component_version" content="{{ @root.page.componentVersion.version }}"/>
